	<layer name="imageWindow" keep="true" type="container" bgcolor="0xffffff" bgalpha="0.4" alpha="0" align="centre" width="600" height="400" visible="false">
		<layer name="imageContainer" keep="true" type="container" bgcolor="0xffffff" bgalpha="0" align="centre"  width="580" height="385">
			<layer name="imageContent" url="" align="centre"  width="580" height="385" onclick="hideImage()"/>
		</layer>
		<layer name="imageCloseBtn" keep="true" url="%SWFPATH%/photo/window_close.png" align="righttop" width="40" height="40" x="-30" y="-30" onclick="hideImage()"/>
	</layer>

	<action name="openMenu">
		tween(layer[menu_container].x, 0);
		set(layer[menu_swich].url, %SWFPATH%/skin/menu_swich_open.png);
		set(layer[menu_swich].onclick, closeMenu());
	</action>

	<action name="closeMenu">
		tween(layer[menu_container].x, -195);
		set(layer[menu_swich].url, %SWFPATH%/skin/menu_swich_close.png);
		set(layer[menu_swich].onclick, openMenu());
	</action>

	<action name="hideImage">
		set(layer[imageWindow].visible, false);
        set(layer[imageWindow].alpha, 0);
	</action>

	<action name="showImage">
		if(layer[imageWindow].visible,
			hideImage();
		);

		set(layer[imageContent].url, %1);
		set(layer[imageWindow].visible, true);
		tween(layer[imageWindow].alpha, 1);
	</action>
