<cursors url="images/qtvr-cursors.png"
  type="8way"
  move="0|0|16|16"
  drag="16|0|16|16"
  arrow_u="32|0|16|16"
  arrow_d="48|0|16|16"
  arrow_l="64|0|16|16"
  arrow_r="80|0|16|16"
  arrow_lu="96|0|16|16"
  arrow_ru="112|0|16|16"
  arrow_rd="128|0|16|16"
  arrow_ld="144|0|16|16"
  />

